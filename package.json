{
  "name": "@mongodb-js/compass-crud",
  "productName": "Compass CRUD plugin",
  "version": "0.4.2",
  "description": "Compass Plugin for CRUD Operations",
  "repository": "https://github.com/10gen/compass-crud",
  "main": "index.js",
  "scripts": {
    "ci": "npm run check && npm test",
    "fmt": "mongodb-js-fmt ./*.js ./test/*.js",
    "check": "mongodb-js-precommit './src/**/*{.js,.jsx}' './test/**/*.js' index.js",
    "compile": "npm run jsx && npm run less",
    "postcompile": "cp -r styles/fonts lib/styles/",
    "jsx": "babel ./src --out-dir ./lib --presets react",
    "preless": "lessc ./styles/compass/index.less ./lib/styles/compass/index.css",
    "less": "lessc ./styles/index.less ./lib/styles/index.css",
    "link-plugin": "./scripts/link.sh",
    "unlink-plugin": "./scripts/unlink.sh",
    "prepublish": "npm run compile",
    "pretest": "npm run compile && mongodb-runner install && mongodb-runner start --port 27018",
    "posttest": "mongodb-runner stop --port 27018",
    "prestart": "npm run compile",
    "start": "electron ./electron",
    "test": "hadron-build test",
    "test-enzyme": "npm test -- --enzyme",
    "test-main": "npm test -- --main",
    "test-renderer": "npm test -- --renderer",
    "test-unit": "npm test -- --unit",
    "test-functional": "npm test -- --functional"
  },
  "precommit": [
    "check"
  ],
  "license": "Apache-2.0",
  "devDependencies": {
    "babel-cli": "^6.10.1",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-preset-react": "^6.11.1",
    "babel-register": "^6.9.0",
    "bootstrap": "https://github.com/twbs/bootstrap/archive/v3.3.5.tar.gz",
    "bson": "^1.0.4",
    "chai": "^3.5.0",
    "chai-as-promised": "^5.1.0",
    "chai-enzyme": "^0.5.0",
    "debug": "^2.6.8",
    "electron": "^1.6.1",
    "electron-mocha": "^3.3.0",
    "enzyme": "^2.4.1",
    "eslint-config-mongodb-js": "^2.1.0",
    "font-awesome": "^4.7.0",
    "hadron-app": "^1.0.0",
    "hadron-app-registry": "^6.0.0",
    "hadron-build": "^13.1.0",
    "hadron-ipc": "0.0.8",
    "hadron-react-buttons": "^1.5.0",
    "hadron-react-components": "^1.9.1",
    "hadron-spectron": "^0.0.1",
    "jsdom": "^9.4.2",
    "less": "^2.7.1",
    "less-loader": "^2.2.3",
    "mocha": "^2.5.3",
    "moment": "^2.18.1",
    "mongodb-connection-model": "^9.2.0",
    "mongodb-data-service": "^7.0.0",
    "mongodb-js-fmt": "^0.0.3",
    "mongodb-js-precommit": "^0.2.8",
    "mongodb-ns": "^2.0.0",
    "mongodb-runner": "^3.5.0",
    "numeral": "^1.5.3",
    "pluralize": "^6.0.0",
    "pre-commit": "^1.1.3",
    "prop-types": "^15.5.8",
    "react": "^15.5.4",
    "react-bootstrap": "0.31.0",
    "react-dom": "^15.5.4",
    "react-fontawesome": "^1.6.1",
    "reflux": "^0.4.1",
    "reflux-state-mixin": "^0.7.0",
    "sinon": "^1.17.6",
    "sinon-chai": "^2.8.0",
    "uuid": "^3.1.0"
  },
  "dependencies": {
    "ag-grid": "12.0.0",
    "ag-grid-react": "12.0.0",
    "hadron-document": "^2.4.0",
    "hadron-react-bson": "^1.4.0",
    "hadron-type-checker": "^3.0.0",
    "jquery": "^2.1.4",
    "lodash": "^4.17.4",
    "marky": "^1.2.0",
    "react-click-outside": "^2.2.0"
  }
}
