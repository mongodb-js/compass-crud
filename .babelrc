{
  "env": {
    "production": {
      "presets": ["minify"]
    },
    "test": {
      "presets": [
        ["es2015"]
      ],
      "plugins": [
        "react-hot-loader/babel",
        "syntax-object-rest-spread",
        "transform-object-rest-spread",
        "transform-class-properties"
      ]
    }
  },
  "presets": [
    "react",
    ["env", {
      "targets": {
